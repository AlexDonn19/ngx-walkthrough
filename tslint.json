{
    "extends": [
        "tslint-angular"
    ],
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            [
                "dir-prefix1",
                "dir-prefix2"
            ],
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            [
                "ngx"
            ],
            "kebab-case"
        ],
        "max-line-length": [false],
        "no-input-rename": [false],
        "no-output-on-prefix": [false],
        "no-output-rename": [false]
    }
}